{
  "name": "bespoke-matchmaking-backend",
  "version": "1.0.0",
  "description": "Backend API for the Bespoke Matchmaking Platform",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback",
    "db:seed": "knex seed:run",
    "docker:build": "docker build -t bespoke-matchmaking-backend .",
    "docker:run": "docker run -p 3000:3000 bespoke-matchmaking-backend"
  },
  "keywords": [
    "matchmaking",
    "dating",
    "api",
    "nodejs",
    "typescript",
    "express"
  ],
  "author": "Bespoke Matchmaking Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "socket.io": "^4.7.4",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "mongoose": "^8.0.3",
    "redis": "^4.6.11",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "uuid": "^9.0.1",
    "speakeasy": "^2.0.0",
    "qrcode": "^1.5.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "express-session": "^1.17.3",
    "connect-redis": "^7.1.0",
    "cron": "^3.1.6",
    "nodemailer-sendgrid-transport": "^0.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/speakeasy": "^2.0.10",
    "@types/qrcode": "^1.5.5",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.14",
    "@types/passport-facebook": "^3.0.3",
    "@types/passport-jwt": "^3.0.13",
    "@types/express-session": "^1.17.10",
    "@types/connect-redis": "^0.0.23",
    "@types/cron": "^2.4.0",
    "@types/node": "^20.10.4",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "typescript": "^5.3.3",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
